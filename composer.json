{
  "name": "kilingzhang/smartqq-sdk-php",
  "description": "SmartQQ 协议 SDK",
  "keywords": [
    "smartqq",
    "webqq",
    "sdk",
    "qq",
    "php-smartqq",
    "php-webqq",
    "php-qq",
    "协议"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "kilingzhang",
      "email": "slight@kilingzhang.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "kilingzhang\\SmartQQ\\": "src/SmartQQ"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kilingzhang\\SmartQQ\\Tests\\": "tests"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
